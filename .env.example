# GitHub credentials
GITHUB_API_TOKEN=your_github_token_here
GITHUB_USERNAME=your_github_username

# Yandex Cloud credentials
YC_OAUTH_TOKEN=your_yandex_oauth_token
YC_CLOUD_ID=your_cloud_id
YC_FOLDER_ID=your_folder_id
YC_SERVICE_ACCOUNT_ID=your_service_account_id
YC_SERVICE_ACCOUNT_KEY_FILE=path/to/key/file.json

# Database credentials (used for creating/accessing databases)
DB_ADMIN_USERNAME=admin_username
DB_ADMIN_PASSWORD=admin_password

# SSH settings
SSH_PRIVATE_KEY_PATH=~/.ssh/id_rsa
SSH_PUBLIC_KEY_PATH=~/.ssh/id_rsa.pub

# Templates settings
TEMPLATE_REPO_URL_DJANGO=https://github.com/yourusername/django-template
TEMPLATE_REPO_URL_REACT=https://github.com/yourusername/react-template

# AI configuration (if using AI agents)
OPENAI_API_KEY=your_openai_api_key

# GUI settings
GUI_THEME=dark  # dark or light 

# Projects settings
PROJECTS_ROOT_DIR=~/projects  # Path to the root directory for all projects 