# GitHub credentials
GITHUB_API_TOKEN=your_github_token_here
GITHUB_USERNAME=your_github_username

# Yandex Cloud credentials
# Service account JSON key (the entire JSON content as text)
YC_SA_JSON_CREDENTIALS={"id":"ajeu4t94gtghnvu5","service_account_id":"...","private_key":"..."}
# Other Yandex Cloud settings
YC_CLOUD_ID=your_cloud_id
YC_FOLDER_ID=your_folder_id
YC_REGISTRY_ID=your_registry_id
YC_SERVICE_ACCOUNT_ID=your_service_account_id
YC_POSTGRES_CLUSTER_ID=your_postgres_cluster_id
YC_NETWORK_ID=your_network_id

# SSH settings
SSH_PRIVATE_KEY_PATH=~/.ssh/id_rsa
SSH_PUBLIC_KEY_PATH=~/.ssh/id_rsa.pub

# AI configuration (if using AI agents)
OPENAI_API_KEY=your_openai_api_key

# GUI settings
GUI_THEME=dark  # dark or light

# Projects settings (path to the root directory for all projects)
PROJECTS_ROOT_DIR=~/projects
